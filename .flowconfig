[ignore]
<PROJECT_ROOT>/packages/admin
<PROJECT_ROOT>/packages/legacy
<PROJECT_ROOT>/build
.*/node_modules/.*

[include]

[libs]

[lints]
sketchy-null=warn

[options]
all=true
emoji=true
suppress_comment= \\(.\\|\n\\)*\\$FlowSkipForTesting
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
sketchy-null